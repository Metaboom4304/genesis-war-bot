{
  "name": "genesis-war-bot",
  "version": "1.0.0",
  "description": "Telegram bot for GENESIS WAR with API services",
  "type": "module",
  "main": "GENESIS_LAUNCHER.js",
  "scripts": {
    "start": "node GENESIS_LAUNCHER.js",
    "start:api": "node index.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:api\"",
    "dev": "nodemon GENESIS_LAUNCHER.js",
    "dev:api": "nodemon index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.61.0",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.3",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "game",
    "map",
    "api",
    "postgresql",
    "postgis"
  ],
  "author": "Genesis Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Metaboom4304/genesis-war-bot"
  },
  "bugs": {
    "url": "https://github.com/Metaboom4304/genesis-war-bot/issues"
  }
}
